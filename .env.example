# OpenRouter API Key (для AI анализа настроения)
# Получить можно на https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Kinopoisk API Key (для получения данных о фильмах)
# Получить можно на https://kinopoisk.dev
KINOPOISK_API_KEY=your_kinopoisk_api_key_here

# Session Secret (для безопасности сессий)
# Любая случайная строка
SESSION_SECRET=your_random_session_secret_here

# Node Environment
NODE_ENV=production
